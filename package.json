{
  "name": "planning-poker",
  "version": "1.0.0",
  "private": true,
  "repository": "https://github.com/Ebonsignori/pp-bot",
  "description": "api of Planning Poker",
  "main": "src/server.js",
  "scripts": {
    "prod": "node src/server.js",
    "prod.up": "docker-compose -f ./docker/docker-compose.production.yml -p pp-api up -d",
    "prod.down": "docker-compose -f ./docker/docker-compose.production.yml -p pp-api down",
    "dev": "nodemon src/server.js",
    "dev.up": "docker-compose -f ./docker/docker-compose.yml -p pp-api up -d",
    "dev.down": "docker-compose -f ./docker/docker-compose.yml -p pp-api down",
    "new-migration": "node-pg-migrate create",
    "migrate": "node-pg-migrate up --ignore-pattern=helpers.js",
    "migrate-down": "node-pg-migrate down --ignore-pattern=helpers.js",
    "test": "npm run test.up && npm run test.migrate && NODE_ENV=test LOGGING_LEVEL=warn mocha --recursive tests --exit && npm run test.down",
    "test.up": "docker-compose -f ./docker/docker-compose.test.yml -p pp-test up -d",
    "test.down": "docker-compose -f ./docker/docker-compose.test.yml -p pp-test down",
    "test.migrate": "node ./docker/wait-for.js 1000 && DATABASE_URL=postgres://root:root@localhost:6545/pp-api-db node-pg-migrate up --ignore-pattern=helpers.js"
  },
  "author": "Evan Bonsignori",
  "license": "ISC",
  "dependencies": {
    "@octokit/app": "^2.2.2",
    "@octokit/rest": "^16.15.0",
    "@octokit/webhooks": "^5.3.3",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.5",
    "chalk": "^2.4.2",
    "connect-ensure-login": "^0.1.1",
    "connect-redis": "^3.4.1",
    "cors": "^2.8.5",
    "cuid": "^2.1.6",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "express-socket.io-session": "^1.3.5",
    "ioredis": "^4.9.0",
    "knex": "^0.16.3",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-slack": "0.0.7",
    "pg": "^7.9.0",
    "request": "^2.88.0",
    "socket.io": "^2.4.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "got": "^9.6.0",
    "mocha": "^6.1.3",
    "node-pg-migrate": "^3.18.1",
    "nodemon": "^1.18.9",
    "redisscan": "^2.0.0",
    "standard": "^12.0.1"
  }
}
